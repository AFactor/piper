<unity xmlns="http://schemas.microsoft.com/practices/2010/unity">

  <!--<alias alias="iNavigationOperation"
           type="Tesco.Com.Pipeline.Operations.IOperation[Navigation],Tesco.Com.Pipeline"/>

  <alias alias="NavigationOperation"
         type="Tesco.Com.Pipeline.Operations.NavigationOperation,Tesco.Com.Pipeline"/>-->

  <alias alias="NavigationEntity"
         type="Tesco.Com.Pipeline.Entities.ResponseEntities.Navigation,Tesco.Com.Pipeline"/>
  
  <alias alias="iNavigationPipeline"
           type="Tesco.Com.Pipeline.Pipe.IPipeline[NavigationEntity],Tesco.Com.Pipeline"/>

  <alias alias="GapiNavigationPipeline"
         type="Tesco.Com.Pipeline.Pipe.NavigationPipeline,Tesco.Com.Pipeline"/>



  

  <container>
    <register type="Tesco.Com.Pipeline.Provider.Contract.IProductBrowseProvider, Tesco.Com.Pipeline"
			        mapTo="Tesco.Com.Pipeline.Provider.GAPI.GAPIProductBrowseProvider, Tesco.Com.Pipeline">
    </register>
    <register type="Tesco.Com.Pipeline.Provider.Contract.IDeviceIdentificationProvider, Tesco.Com.Pipeline"
			        mapTo="Tesco.Com.Pipeline.Provider.Appstore.AppstoreDeviceIdentificationProvider, Tesco.Com.Pipeline">
    </register>
    <register type="Tesco.Com.Pipeline.Provider.Contract.IPriceProvider, Tesco.Com.Pipeline"
			        mapTo="Tesco.Com.Pipeline.Provider.GAPI.GAPIPriceProvider, Tesco.Com.Pipeline">
    </register>
    <register type="Tesco.Com.Pipeline.Provider.Contract.IPromotionProvider, Tesco.Com.Pipeline"
			        mapTo="Tesco.Com.Pipeline.Provider.GAPI.GAPIPromotionProvider, Tesco.Com.Pipeline">
    </register>
    <register type="Tesco.Com.Pipeline.Provider.Contract.IProductProvider, Tesco.Com.Pipeline"
			        mapTo="Tesco.Com.Pipeline.Provider.GAPI.GAPIProductProvider, Tesco.Com.Pipeline">
    </register>
    <!--<register type="Tesco.Com.Pipeline.Operations.Contract.INavigationOperation, Tesco.Com.Pipeline"
			        mapTo="Tesco.Com.Pipeline.Operations.NavigationOperation, Tesco.Com.Pipeline">
    </register>-->

    <register type="Tesco.Com.Pipeline.Pipe.IPipeline[],Tesco.Com.Pipeline" mapTo="Tesco.Com.Pipeline.Pipe.BasePipeline[],Tesco.Com.Pipeline"></register>
    <!--make the aliases work-->
    <register type="Tesco.Com.Pipeline.Pipe.IPipeline[NavigationEntity],Tesco.Com.Pipeline" mapTo="Tesco.Com.Pipeline.Pipe.NavigationPipeline,Tesco.Com.Pipeline"></register>
    
    
    <register type="Tesco.Com.Pipeline.Operations.IOperation[],Tesco.Com.Pipeline" mapTo="Tesco.Com.Pipeline.Operations.BaseOperation[],Tesco.Com.Pipeline"></register>
    <register type="Tesco.Com.Pipeline.Operations.IOperation[NavigationEntity],Tesco.Com.Pipeline" mapTo="Tesco.Com.Pipeline.Operations.NavigationOperation,Tesco.Com.Pipeline"></register>
    
    
  </container>

  
</unity>